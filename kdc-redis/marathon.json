{
  "id": "kdc-redis",
  "instances": 1,
  "cpus": 0.25,
  "mem": 128.0,
  "container": {
    "docker": {
      "type": "DOCKER",
      "image": "hub.kungfudev.io/kdc-redis:latest",
      "network": "BRIDGE",
      "parameters": [],
      "portMappings": [
        {
          "containerPort": 6379,
          "hostPort": 6379,
          "protocol": "tcp"
        }
      ]
    },
    "volumes": [
      {
        "hostPath": "/data/kdc-redis/storage",
        "containerPath": "/data",
        "mode": "RW"
      }
    ]
  },
  "uris": [
    "file:///etc/.dockercfg"
  ],
  "env": {
    "SETTINGS_FLAVOR": "local"
  }
}