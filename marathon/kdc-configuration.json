{
  "id": "kdc-configuration",
  "instances": 1,
  "cpus": 0.25,
  "mem": 128.0,
  "container": {
    "docker": {
      "type": "DOCKER",
      "image": "hub.kungfudev.io/kungfudev-cloud-configuration:latest",
      "network": "BRIDGE",
      "parameters": [],
      "portMappings": [
        {
          "containerPort": 8888,
          "hostPort": 8888,
          "protocol": "tcp"
        }
      ]
    }
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "path": "/health",
      "portIndex": 0
    }
  ],
  "uris": [
    "file:///etc/.dockercfg"
  ],
  "env": {
    "SETTINGS_FLAVOR": "local"
  }
}